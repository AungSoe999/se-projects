<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox spacing="20.0" stylesheets="@application.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="com.jdc.student.controller.ClassList">
   <styleClass>
      <String fx:value="ContentPane" />
      <String fx:value="Padding20" />
   </styleClass>
   <children>
      <HBox spacing="10.0">
         <children>
            <ComboBox fx:id="courseCombo" prefHeight="27.0" prefWidth="211.0" promptText="Course" />
            <DatePicker fx:id="startDate" />
         </children>
      </HBox>
      <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn prefWidth="196.0" text="Course Name" />
          <TableColumn prefWidth="141.0" text="Start Date" />
            <TableColumn prefWidth="144.0" text="Times" />
            <TableColumn prefWidth="148.0" text="Fees" />
            <TableColumn prefWidth="229.0" text="Days" />
        </columns>
      </TableView>
   </children>
</VBox>
