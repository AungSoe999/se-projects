<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<HBox styleClass="MainFrame" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jdc.student.controller.MainFrame">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" styleClass="LeftMenu">
         <children>
            <HBox styleClass="ApplicationTitle">
               <children>
                  <Label text="JDC" />
               </children>
            </HBox>
            <VBox fx:id="menuWrapper" styleClass="MenuWrapper">
               <children>
                  <Label prefHeight="40.0" prefWidth="244.0" text="Registration List" />
                  <Label layoutX="30.0" layoutY="30.0" prefHeight="40.0" prefWidth="244.0" text="New Registration" />
                  <Label layoutX="30.0" layoutY="63.0" prefHeight="40.0" prefWidth="244.0" text="Student List" />
                  <Label layoutX="30.0" layoutY="130.0" prefHeight="40.0" prefWidth="244.0" text="Course List" />
                  <Label layoutX="30.0" layoutY="180.0" prefHeight="40.0" prefWidth="244.0" text="Add Course" />
                  <Label layoutX="30.0" layoutY="230.0" prefHeight="40.0" prefWidth="244.0" text="Class List" />
                  <Label layoutX="30.0" layoutY="280.0" prefHeight="40.0" prefWidth="244.0" text="Add Class" />
               </children>
            </VBox>
         </children>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <HBox styleClass="TitlePane">
               <children>
                  <Label fx:id="title" text="Student Registration" />
               </children>
            </HBox>
            <StackPane fx:id="contentView" prefHeight="150.0" prefWidth="200.0" styleClass="ContentPane" VBox.vgrow="ALWAYS" />
            <HBox styleClass="MessagePane">
               <children>
                  <Label fx:id="message" text="Message" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</HBox>
