<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.VBox?>

<VBox stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jdc.student.controller.CourseList">
	<styleClass>
		<String fx:value="ContentPane" />
		<String fx:value="Padding20" />
	</styleClass>
	<children>
		<TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
			<columns>
				<TableColumn prefWidth="235.0" text="Name">
					<cellValueFactory>
						<PropertyValueFactory property="name" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn prefWidth="164.0" text="Fees">
					<cellValueFactory>
						<PropertyValueFactory property="feesStr" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn prefWidth="127.0" text="Duration">
					<cellValueFactory>
						<PropertyValueFactory property="durationStr" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn prefWidth="325.0" text="Description">
					<cellValueFactory>
						<PropertyValueFactory property="description" />
					</cellValueFactory>
				</TableColumn>
			</columns>
		</TableView>
	</children>
</VBox>
