<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9">
   <stylesheets>
      <URL value="@color.css" />
      <URL value="@layout.css" />
   </stylesheets>
   <styleClass>
      <String fx:value="contentView" />
      <String fx:value="back1" />
   </styleClass>
   <children>
      <HBox styleClass="searchForm">
         <children>
            <VBox styleClass="formGroup">
               <children>
                  <Label text="Date From" />
                  <DatePicker />
               </children>
            </VBox>
            <VBox layoutX="20.0" layoutY="20.0" styleClass="formGroup">
               <children>
                  <Label text="Date To" />
                  <DatePicker />
               </children>
            </VBox>
            <VBox layoutX="224.0" layoutY="20.0" styleClass="formGroup">
               <children>
                  <Label text="Name" />
                  <TextField />
               </children>
            </VBox>
            <VBox layoutX="428.0" layoutY="20.0" styleClass="formGroup">
               <children>
                  <Label />
                  <Button mnemonicParsing="false" text="Search">
                     <styleClass>
                        <String fx:value="back3" />
                        <String fx:value="text1" />
                     </styleClass>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
      <TableView prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn prefWidth="66.0" text="ID" />
          <TableColumn prefWidth="147.0" text="Room" />
            <TableColumn prefWidth="140.0" text="Reserve Date" />
            <TableColumn prefWidth="140.0" text="Check In Date" />
            <TableColumn prefWidth="140.0" text="Check Out Date" />
            <TableColumn prefWidth="183.0" text="Customer Name" />
            <TableColumn prefWidth="157.0" text="Phone" />
            <TableColumn prefWidth="202.0" text="Email" />
        </columns>
      </TableView>
   </children>
</VBox>
