<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox styleClass="ContentView" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="com.jdc.ishop.controller.SaleReport">
   <stylesheets>
      <URL value="@color.css" />
      <URL value="@layout.css" />
   </stylesheets>
   <children>
      <HBox>
         <children>
            <ComboBox fx:id="schCategory" prefHeight="27.0" prefWidth="189.0" promptText="Category" />
            <DatePicker fx:id="schDateFrom" promptText="Date From" />
            <DatePicker fx:id="schDateTo" promptText="Date To" />
            <TextField fx:id="schEmployee" promptText="Sale Employee" HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" onAction="#clear" text="CLEAR" />
         </children>
         <styleClass>
            <String fx:value="Padding10" />
            <String fx:value="Space10" />
         </styleClass>
      </HBox>
      <VBox styleClass="Padding10" VBox.vgrow="ALWAYS">
         <children>
            <TableView onSort="#table" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn prefWidth="89.0" text="Order ID" />
                <TableColumn prefWidth="153.0" text="Sale Person" />
                  <TableColumn prefWidth="166.0" text="Category" />
                  <TableColumn prefWidth="176.0" text="Item" />
                  <TableColumn prefWidth="84.0" text="Unit Price" />
                  <TableColumn prefWidth="83.0" text="Count" />
                  <TableColumn prefWidth="127.0" text="Total" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</VBox>
