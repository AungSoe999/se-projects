<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox styleClass="contentView" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1">
   <stylesheets>
      <URL value="@color.css" />
      <URL value="@layout.css" />
   </stylesheets>
   <children>
      <HBox VBox.vgrow="ALWAYS">
         <styleClass>
            <String fx:value="padd20" />
            <String fx:value="space10" />
         </styleClass>
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <styleClass>
                  <String fx:value="width300" />
                  <String fx:value="space10" />
               </styleClass>
               <children>
                  <HBox>
                     <children>
                        <Label styleClass="fs20" text="Category" />
                     </children>
                     <styleClass>
                        <String fx:value="ImportantBack" />
                        <String fx:value="padd20" />
                     </styleClass>
                  </HBox>
                  <ListView prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                  <Button mnemonicParsing="false" text="Add Product">
                     <styleClass>
                        <String fx:value="width300" />
                        <String fx:value="HilightBack" />
                        <String fx:value="HilightText" />
                        <String fx:value="padd20" />
                        <String fx:value="fs20" />
                     </styleClass>
                  </Button>
               </children>
            </VBox>
            <VBox layoutX="30.0" layoutY="30.0" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
               <styleClass>
                  <String fx:value="width300" />
                  <String fx:value="space10" />
               </styleClass>
               <children>
                  <HBox>
                     <children>
                        <Label styleClass="fs20" text="Products" />
                     </children>
                     <styleClass>
                        <String fx:value="ImportantBack" />
                        <String fx:value="padd20" />
                     </styleClass>
                  </HBox>
                  <ScrollPane hbarPolicy="NEVER" VBox.vgrow="ALWAYS">
                     <content>
                        <FlowPane prefHeight="200.0" prefWidth="200.0" />
                     </content>
                  </ScrollPane>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
