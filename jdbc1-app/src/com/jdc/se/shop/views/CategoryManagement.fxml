<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1">
   <stylesheets>
      <URL value="@color.css" />
      <URL value="@layout.css" />
   </stylesheets>
   <styleClass>
      <String fx:value="contentView" />
      <String fx:value="padd20" />
      <String fx:value="space10" />
   </styleClass>
   <children>
      <FlowPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
      <HBox>
         <children>
            <Button mnemonicParsing="false" text="Add New">
               <styleClass>
                  <String fx:value="width300" />
                  <String fx:value="HilightBack" />
                  <String fx:value="HilightText" />
                  <String fx:value="fs20" />
               </styleClass>
            </Button>
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Upload Category">
               <styleClass>
                  <String fx:value="width300" />
                  <String fx:value="HilightBack" />
                  <String fx:value="HilightText" />
                  <String fx:value="fs20" />
               </styleClass>
            </Button>
         </children>
         <styleClass>
            <String fx:value="space10" />
            <String fx:value="right" />
         </styleClass>
      </HBox>
   </children>
</VBox>
